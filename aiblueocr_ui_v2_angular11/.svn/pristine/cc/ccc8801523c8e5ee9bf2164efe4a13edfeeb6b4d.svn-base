<div class="chartBtnArea mT15">
	<div class="chartBtn  mB10">
		<a href="javascript:void(0);" (click)="search()"  class="btn btn-default" title="새로고침"><i class="ti-loop"></i></a>
		<a href="javascript:void(0)" (click)="evtBtnSaveClick()" class="btn btn-primary" title="선택">선택</a>
	</div>														
</div>
<div class="algorithmWrap">
	<div class="algorithm01"><!--col60-->
		<div class="algorithmSelect">
			<div *ngFor="let code of aiTypeList; let i = index;" class="algorithmSelectList algorithmCon0{{i+1}}">
				<h3>{{code.CODE_NM}}
					<span class="mL5"><a href="javascript:void(0);" (click)="openAiAlgParameterModal(code.CODE_ID, '')" class="algorithmSearch"><i class="ti-search"></i></a></span>
				</h3>
				<ul class="algorithmSelectCon mCustomScrollbar">
					<ng-container *ngFor="let item of dataList">
					<li *ngIf="item.MODEL_TYPE === code.CODE_ID">
						<dl>
							<dt>
								<div class="checkbox-out">
								<input  type="checkbox" id="algorithmCheck{{item.ALG_ID}}" [checked]="item.CHECK" value="{{item.ALG_ID}}"/><label for="algorithmCheck{{item.ALG_ID}}" class="checkbox">{{item.ALG_NM}}</label> 
								</div>
							</dt>
							<dd><!-- <a href="javascript:void(0);" (click)="openAiAlgParameterModal(item.MODEL_TYPE, item.ALG_ID)" class="algorithmSearch"><i class="ti-search"></i></a> --></dd>
						</dl>
					</li>
					</ng-container>
				</ul>
			</div>
		</div>
	</div>
</div>
